model NavMain {
    id                      String          @id                 @default(uuid())
    createdAt               DateTime                            @default(now())
    updatedAt               DateTime                            @updatedAt
    active                  Boolean                             @default(true)

    title                   String
    url                     String

    parentId                String?

    parent                  NavMain?                            @relation("NavHierarchy", references: [id], fields: [parentId])
    children                NavMain[]                           @relation("NavHierarchy")

    userRelNavMain          UserRelNavMain[]
}