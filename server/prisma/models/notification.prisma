model Notification {
    id                      String          @id                 @default(uuid())
    createdAt               DateTime                            @default(now())
    updatedAt               DateTime                            @updatedAt
    active                  Boolean                             @default(true)

    read                    Boolean                             @default(false)
    title                   String
    message                 String
    userId                  String

    user                    User                                @relation(fields: [userId], references: [id])
}